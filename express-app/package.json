{
  "name": "expressapp",
  "version": "0.0.0",
  "private": true,
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec 'ts-node ./src/index.ts'",
    "tsc": "tsc",
    "migrate": "npx sequelize-cli db:migrate",
    "undomigrate": "npx sequelize-cli db:migrate:undo",
    "cpCJS": "cp -r ./db/models ./dist/db"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cls-hooked": "^4.2.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "pino-http": "^9.0.0",
    "pino-pretty": "^10.3.1",
    "sequelize": "^6.35.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.1",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "nodemonConfig": {
    "watch": [
      "./auth",
      "./config",
      "./controllers",
      "./db",
      "./middleware",
      "./routers",
      "./src"
    ],
    "ext": "ts,json,js"
  }
}
